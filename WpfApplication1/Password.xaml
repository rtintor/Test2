<Window x:Class="Bimbo.DSD.UpdateAdmin.Password"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Password" Height="150" Width="250">
    <Grid>
        <Label Content="Password" Height="28" HorizontalAlignment="Left" Margin="12,14,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="28" HorizontalAlignment="Right" Margin="0,14,29,0" Name="txtPass" VerticalAlignment="Top" Width="120" />
        <Button Content="Ok" Height="23" HorizontalAlignment="Left" Margin="124,59,0,0" Name="btnOk" VerticalAlignment="Top" Width="75" Click="btnOk_Click" />
        <Button Content="Cancelar" Height="23" HorizontalAlignment="Left" Margin="43,59,0,0" Name="btnCancel" VerticalAlignment="Top" Width="75" Click="btnCancel_Click" />
    </Grid>
</Window>
